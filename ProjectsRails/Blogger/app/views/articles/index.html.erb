<h1>Listing Articles</h1>
<%= link_to 'My blogger', controller: 'articles'%>	
<%= link_to 'New Article', new_article_path %>	
<table>
	<thead>
		<tr>
			<th>Title</th>
			<th>text</th>
		</tr>
	</thead>
	<tbody>
		<% @article.each do |art| %>
		<tr>
			<td><%= art.title %></td>
			<td><%= art.text %></td>
			<td><%= link_to 'Show', article_path(art) %></td>
			<td><%= link_to 'Edit', edit_article_path(art) %></td>
		</tr>
		<% end %>
	</tbody>
</table>